{
  "name": "moollm",
  "displayName": "Moo_LLM",
  "publisher": "user",
  "description": "Moo_LLM is a sidebar-based code assistant with model switching, tool calling, and chat UI.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "activationEvents": [
    "onView:moollm.chatView"
  ],
  "contributes": {
    "viewsContainers": {
      "panel": [
        {
          "id": "moollm-chat-panel-container",
          "title": "Moo_LLM",
          "icon": "$(comment-discussion)"
        }
      ]
    },
    "views": {
      "moollm-chat-panel-container": [
        {
          "id": "moollm.chatView",
          "name": "Chat",
          "type": "webview",
          "when": "true"
        }
      ]
    },
    "commands": [
      {
        "command": "moollm.openChat",
        "title": "Open Moo Chat",
        "category": "Moo_LLM"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npx vsce package"
  },
  "devDependencies": {
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "@google/genai": "^1.5.1",
    "@google/generative-ai": "^0.24.1",
    "highlight.js": "^11.11.1",
    "marked": "^15.0.12",
    "node-fetch": "^3.3.2"
  }
}
